#Task: 01
lista = []
while True:
    u = input()
    if u=="STOP":
        break
    lista.append(int(u))

track = []
for i in range (len(lista)):
    if lista[i] not in track:
        count = 0
        for j in range(len(lista)):
            if lista[i] == lista[j]:
                count +=1
        track.append(lista[i])
        print(f"{track[-1]} - {count} times")
